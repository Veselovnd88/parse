def acc(defs):
    end = defs.find('span')
    length = 4
    if end == -1:
        end = defs.find('span)')
        length = 5
        if end == -1:
            end = defs.find('scale)')
            length = 6
    return defs[:end+length]

def table(defs):
    parameters = {}

    parameters['Accuracy'] = acc(defs)
    parameters['Dial Size and Case Material']=dial(defs)
    return parameters





